        <section class="listagem-produtos-home">
            <% produtosIndex.forEach((produto, i) =>  { %> 
           <div class="div-listagem-produtos-home" id="produto-<%=i%>" >
                <div class="div-up-produtos-home">
                    <a href="/produto"><img id="imagemProduto" src="images/<%=produto.categoria%>/<%=produto.imagem%>" alt=""></a>
                </div>
                <div class="div-down-produtos-home">
                    <div class="titulo-produto">
                        <h5><%=produto.nome%> </h5>                    
                    </div>
                    <div class="descricao-produto-home">
                        <p><%=produto.descricao%></p>
                    </div>
                    <div class="price-produto-listagem-produto">
                        <div class="de-por-price">
                            <h3>De: R$ <%=produto.valorSemDesconto%> </h3>
                        </div>
                        <div class="price-real">
                            <h3>Por:  <span id="valorProduto"> R$ <%=produto.valorVenda%></span> </h3>
                        </div>
                    </div>
                    <div class="cta-produtos">
                        <button id="btnAddCarrinho" class="add-carrinho addCarrinhoPrimary">
